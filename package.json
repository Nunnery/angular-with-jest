{
  "name": "angular-with-jest",
  "version": "0.0.0",
  "license": "MIT",
  "dependencies": {
    "@uirouter/angularjs": "^1.0.5",
    "@uirouter/dsr": "^1.0.2",
    "@uirouter/sticky-states": "^1.4.1",
    "@uirouter/visualizer": "^5.0.0",
    "angular": "^1.6.2"
  },
  "devDependencies": {
    "@types/jest": "^21.1.0",
    "angular-mocks": "^1.6.5",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-angularjs-annotate": "^0.8.0",
    "babel-preset-env": "^1.6.0",
    "clean-webpack-plugin": "^0.1.16",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "del-cli": "^1.1.0",
    "eslint": "^4.2.0",
    "eslint-config-angular": "^0.5.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-angular": "^3.0.0",
    "eslint-plugin-prettier": "^2.1.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.29.0",
    "husky": "^0.14.3",
    "jest-cli": "^21.0.0",
    "jest-junit": "^3.0.0",
    "lint-staged": "^4.0.2",
    "ng-annotate-loader": "^0.6.1",
    "node-sass": "^4.5.3",
    "npm-watch": "^0.3.0",
    "opn-cli": "^3.1.0",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.5.3",
    "sass-loader": "^6.0.6",
    "serve": "^6.0.2",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.1",
    "webpack": "^4.16.4",
    "webpack-dev-server": "^2.5.1",
    "webpack-fail-plugin": "^2.0.0",
    "webpack-manifest-plugin": "^1.2.0",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(html)$": "<rootDir>/__mocks__/htmlMock.js"
    },
    "roots": [
      "<rootDir>/src/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/jest.init.js",
    "testResultsProcessor": "./node_modules/jest-junit"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add",
      "jest --bail --findRelatedTests"
    ]
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "clean": "del dist",
    "coverage": "npm test -- --coverage",
    "lint": "eslint src",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "precommit": "lint-staged",
    "prettier": "prettier --single-quote --write \"src/**/*.js\"",
    "start": "cross-env NODE_ENV=development webpack-dev-server --open",
    "start:dist": "npm run build && serve dist --open",
    "test": "cross-env JEST_JUNIT_OUTPUT=${JEST_JUNIT_OUTPUT:=./coverage/junit.xml} jest",
    "watch": "npm-watch"
  },
  "watch": {
    "test": "{src,test}/*.js",
    "lint": "{src,test}/*.js",
    "build": "src"
  }
}
